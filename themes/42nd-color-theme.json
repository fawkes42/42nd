        "list.hoverBackground": "#846daf1a",
        "list.activeSelectionBackground": "#846daf4d",
        "list.activeSelectionForeground": "#f4f4f4",
        "list.filterMatchBorder": "#373737",
        "list.inactiveSelectionBackground": "#846daf3d",
        "input.background": "#0e0d0d",
        "input.foreground": "#f4f4f4",
        "input.border": "#373737",
        "input.placeholderForeground": "#8c8c8c",
        "inputOption.activeBackground": "#846daf",

{
    "name": "42nd",
    "type": "dark",
    "colors": {
        "activityBar.background": "#0e0d0d",
        "activityBar.border": "#0e0d0d",
        "activityBarBadge.background": "#9e4b4b",
        "activityBarBadge.foreground": "#f4f4f4",
        "editor.background": "#0f0f0f",
        "editor.findMatchBackground": "#cab577",
        "editor.findMatchHighlightBackground": "#cab5778a",
        "editor.foreground": "#f4f4f4",
        "editor.inactiveSelectionBackground": "#9e4b4b8a",
        "editor.lineHighlightBackground": "#fdbe0013",
        "editor.selectionBackground": "#fdbe0027",
        "editor.selectionForeground": "#f4f4f4",
        "editorWhitespace.foreground": "#3b3b3b",
        "focusBorder": "#00000000",
        "list.hoverBackground": "#fdbe0013",
        "list.filterMatchBorder": "#00000000",
        "list.activeSelectionBackground": "#fdbe0027",
        "list.activeSelectionForeground": "#f4f4f4",
        "list.focusBackground": "#fdbe0027",
        "list.highlightForeground": "#f4f4f4",
        "list.inactiveSelectionBackground": "#9e4b4b8a",
        "list.inactiveSelectionForeground": "#f4f4f4",
        "panel.background": "#0f0f0f",
        "scrollbar.shadow": "#00000000",
        "scrollbarSlider.background": "#1e1e1e",
        "scrollbarSlider.hoverBackground": "#292929",
        "sideBar.background": "#0e0d0d",
        "sideBar.border": "#0e0d0d",
        "sideBarSectionHeader.background": "#0e0d0d",
        "sideBarSectionHeader.foreground": "#8c8c8c",
        "sideBarTitle.foreground": "#a8a8a8",
        "statusBar.background": "#0e0d0d",
        "statusBar.border": "#0e0d0d",
        "statusBar.foreground": "#f4f4f4",
        "statusBarItem.hoverBackground": "#292726",
        "tab.activeBackground": "#0f0f0f",
        "tab.inactiveBackground": "#0e0d0d",
        "terminal.ansiBlack": "#0e0d0d",
        "terminal.ansiBlue": "#5085c0",
        "terminal.ansiCyan": "#68d5d5",
        "terminal.ansiGreen": "#72c572",
        "terminal.ansiMagenta": "#846daf",
        "terminal.ansiRed": "#9e4b4b",
        "terminal.ansiWhite": "#f4f4f4",
        "terminal.ansiYellow": "#ccb577",
        "terminal.background": "#0f0f0f",
        "titleBar.activeBackground": "#0e0d0d",
        "titleBar.activeForeground": "#f4f4f4",
        "titleBar.border": "#0e0d0d"
    },
    "tokenColors": [
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block"
            ],
            "settings": {
                "foreground": "#8c8c8c"
            }
        },
        {
            "name": "Import/Export Paths",
            "scope": [
                "string.quoted.module",
                "string.quoted.single.module",
                "string.quoted.double.module"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "JSX/TSX Tag",
            "scope": [
                "entity.name.tag.tsx",
                "entity.name.tag.jsx",
                "support.class.component.tsx",
                "support.class.component.jsx"
            ],
            "settings": {
                "foreground": "#846daf"
            }
        },
        {
            "name": "JSX/TSX Attribute Name",
            "scope": [
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.jsx",
                "meta.tag.attributes meta.attribute"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "JSX/TSX Attribute Value",
            "scope": [
                "meta.tag.attributes string.quoted.tsx",
                "meta.tag.attributes string.quoted.jsx"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "Global Operators and Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.bracket",
                "punctuation.section",
                "meta.brace",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.spread",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.bitwise",
                "keyword.operator.ternary"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "TypeScript/JavaScript Specific Operators",
            "scope": [
                "keyword.operator.instanceof.js",
                "keyword.operator.instanceof.ts",
                "keyword.operator.typeof.js",
                "keyword.operator.typeof.ts",
                "keyword.operator.in.js",
                "keyword.operator.in.ts",
                "keyword.operator.of.js",
                "keyword.operator.of.ts"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Global Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "storage.control"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#72c5aa"
            }
        },
        {
            "name": "Function Calls and Methods",
            "scope": [
                "meta.function-call",
                "meta.method-call",
                "support.function.call",
                "entity.name.function.call"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "Support Functions and Types",
            "scope": [
                "support.function",
                "support.type",
                "support.class",
                "support.module"
            ],
            "settings": {
                "foreground": "#72c5aa"
            }
        },
        {
            "name": "Object Properties",
            "scope": [
                "meta.object-literal.key",
                "support.type.property-name",
                "entity.name.tag.yaml",
                "meta.object-literal.key.js",
                "meta.object-literal.key.ts"
            ],
            "settings": {
                "foreground": "#cab577"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "variable.other.object",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#cab577"
            }
        },
        {
            "name": "Class and Type Definitions",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.type.class",
                "support.class"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "Imports and Modules",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "meta.import",
                "meta.export"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "This and Self Keywords",
            "scope": [
                "variable.language.this",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Method Definitions",
            "scope": [
                "entity.name.function",
                "meta.definition.method"
            ],
            "settings": {
                "foreground": "#846daf"
            }
        }
    ]
}
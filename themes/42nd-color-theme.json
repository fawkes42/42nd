        "menu.background": "#0e0d0d",
        "menu.foreground": "#f4f4f4",
        "menu.selectionBackground": "#846daf4d",
        "menu.separatorBackground": "#373737",
        "menu.border": "#373737",
        "menubar.selectionBackground": "#846daf4d",
        "quickInput.background": "#0e0d0d",
        "quickInput.foreground": "#f4f4f4",
        "quickInputTitle.background": "#846daf",
        "panel.background": "#0e0d0d",
        "pickerGroup.border": "#373737",
        "scrollbar.shadow": "#0e0d0d",
        "scrollbarSlider.background": "#846daf3d",
        "scrollbarSlider.hoverBackground": "#846daf4d",
        "sideBar.background": "#0e0d0d",
        "sideBar.border": "#373737",
        "sideBarSectionHeader.background": "#0e0d0d",
        "statusBar.background": "#0e0d0d",
        "statusBar.border": "#373737",
        "statusBarItem.hoverBackground": "#846daf1a",
        "tab.activeBackground": "#0e0d0d",
        "tab.activeBorder": "#0e0d0d",
        "tab.inactiveBackground": "#0e0d0d",
        "terminal.background": "#0e0d0d",
        "titleBar.activeBackground": "#0e0d0d",
        "titleBar.border": "#373737",
        "terminal.ansiBlack": "#0e0d0d",
        "terminal.ansiBrightBlack": "#373737",
        "terminal.ansiRed": "#9e4b4b",
        "terminal.ansiBrightRed": "#ff5c5c",
        "terminal.ansiGreen": "#72c572",
        "terminal.ansiBrightGreen": "#8ce88c",
        "terminal.ansiYellow": "#cab577",
        "terminal.ansiBrightYellow": "#fce788",
        "terminal.ansiBlue": "#5085c0",
        "terminal.ansiBrightBlue": "#6da6ff",
        "terminal.ansiMagenta": "#846daf",
        "terminal.ansiBrightMagenta": "#a57ede",
        "terminal.ansiCyan": "#72c5aa",
        "terminal.ansiBrightCyan": "#89f2d6",
        "terminal.ansiWhite": "#f4f4f4",
        "terminal.ansiBrightWhite": "#ffffff"
    },
    "tokenColors": [
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.line",
                "comment.block"
            ],
            "settings": {
                "foreground": "#8c8c8c"
            }
        },
        {
            "name": "Import/Export Paths",
            "scope": [
                "string.quoted.module",
                "string.quoted.single.module",
                "string.quoted.double.module"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "JSX/TSX Tag",
            "scope": [
                "entity.name.tag.tsx",
                "entity.name.tag.jsx",
                "support.class.component.tsx",
                "support.class.component.jsx"
            ],
            "settings": {
                "foreground": "#846daf"
            }
        },
        {
            "name": "JSX/TSX Attribute Name",
            "scope": [
                "entity.other.attribute-name.tsx",
                "entity.other.attribute-name.jsx",
                "meta.tag.attributes meta.attribute"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "JSX/TSX Attribute Value",
            "scope": [
                "meta.tag.attributes string.quoted.tsx",
                "meta.tag.attributes string.quoted.jsx"
            ],
            "settings": {
                "foreground": "#72c572"
            }
        },
        {
            "name": "Global Operators and Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.bracket",
                "punctuation.section",
                "meta.brace",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.spread",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.bitwise",
                "keyword.operator.ternary"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "TypeScript/JavaScript Specific Operators",
            "scope": [
                "keyword.operator.instanceof.js",
                "keyword.operator.instanceof.ts",
                "keyword.operator.typeof.js",
                "keyword.operator.typeof.ts",
                "keyword.operator.in.js",
                "keyword.operator.in.ts",
                "keyword.operator.of.js",
                "keyword.operator.of.ts"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Global Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "storage.control"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#72c5aa"
            }
        },
        {
            "name": "Function Calls and Methods",
            "scope": [
                "meta.function-call",
                "meta.method-call",
                "support.function.call",
                "entity.name.function.call"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "Support Functions and Types",
            "scope": [
                "support.function",
                "support.type",
                "support.class",
                "support.module"
            ],
            "settings": {
                "foreground": "#72c5aa"
            }
        },
        {
            "name": "Object Properties",
            "scope": [
                "meta.object-literal.key",
                "meta.object-literal.key.js",
                "meta.object-literal.key.ts",
                "support.type.property-name.js",
                "support.type.property-name.ts"
            ],
            "settings": {
                "foreground": "#00c2a8"
            }
        },
        {
            "name": "JSON Property Names",
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml",
                "meta.object-literal.key.json"
            ],
            "settings": {
                "foreground": "#cab577"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "variable.other.object",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#cab577"
            }
        },
        {
            "name": "Class and Type Definitions",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.type.class",
                "support.class"
            ],
            "settings": {
                "foreground": "#5085c0"
            }
        },
        {
            "name": "Imports and Modules",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "meta.import",
                "meta.export"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "This and Self Keywords",
            "scope": [
                "variable.language.this",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#9e4b4b"
            }
        },
        {
            "name": "Method Definitions",
            "scope": [
                "entity.name.function",
                "meta.definition.method"
            ],
            "settings": {
                "foreground": "#846daf"
            }
        }
    ]
}